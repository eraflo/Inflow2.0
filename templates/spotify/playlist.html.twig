{% extends 'base.html.twig' %}

{% block title %}Inflow{% endblock %}

{% block body %}


<a href='{{resultat.uri}}' class="container">
    <div class="row">
        <div class="col-12">
            <h1> {{resultat.name}} </h1>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <img src="{{resultat.images[0].url}}" alt="">
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <p> {{resultat.description}} </p>
        </div>
    </div>
</a>





{% endblock %}
